{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "cd .. && husky install",
    "prepare": "cd .. && husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.24.0",
    "@types/lodash.xor": "^4.5.6",
    "dotenv": "^16.0.0",
    "graphql": "^15.0.0",
    "joi": "^17.6.0",
    "lodash.xor": "^4.5.0",
    "next": "12.1.0",
    "next-auth": "^3.27.0",
    "next-pwa": "^5.4.6",
    "next-seo": "^5.1.0",
    "nextjs-progressbar": "^0.0.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.40",
    "@types/react-slick": "^0.23.8",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.33.0",
    "apollo": "^2.33.9",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.6",
    "cypress": "^9.5.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "jest-styled-components": "^7.0.6",
    "lint-staged": "^12.3.5",
    "msw": "^0.38.2",
    "node-fetch": "^2.6.1",
    "plop": "^3.0.5",
    "prettier": "2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.4"
  }
}
